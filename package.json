{
  "private": true,
  "scripts": {
    "start": "run-p --race watch firefox",
    "firefox": "npm run web-ext -- run",
    "compile": "npm run rollup -- --config",
    "watch": "npm run compile -- --watch",
    "build": "run-s compile \"web-ext -- build\" \"web-ext -- lint\"",
    "build:chrome": "cross-env BROWSER=chrome npm run build",
    "build:firefox": "cross-env BROWSER=firefox npm run build",
    "png-icons": "sucrase-node scripts/png-icons.js",
    "rollup": "sucrase-node node_modules/.bin/rollup",
    "web-ext": "sucrase-node node_modules/.bin/web-ext",
    "flow": "flow",
    "flow:check": "flow check",
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier --check \"**/*.{css,js,md,yml}\"",
    "prettier:fix": "prettier --write \"**/*.{css,html,js,md,yml}\"",
    "test": "run-s eslint prettier flow:check build:chrome build:firefox"
  },
  "dependencies": {
    "n-ary-huffman": "4.0.0",
    "preact": "8.4.2",
    "preact-shadow-root": "1.0.2",
    "tiny-decoders": "1.0.0",
    "webextension-polyfill": "0.4.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "cross-env": "5.2.0",
    "cross-spawn": "6.0.5",
    "eslint": "5.14.0",
    "eslint-config-lydell": "13.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-simple-import-sort": "3.0.0",
    "flow-bin": "0.93.0",
    "mkdirp": "0.5.1",
    "npm-run-all": "4.1.5",
    "optional-require": "1.0.0",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "rollup": "1.2.0",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-prettier": "0.6.0",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-sucrase": "2.1.0",
    "web-ext": "2.9.3",
    "write": "1.0.3"
  }
}
