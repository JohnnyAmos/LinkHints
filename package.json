{
  "private": true,
  "scripts": {
    "start": "run-p --race watch run",
    "run": "npm run web-ext -- run",
    "compile": "npm run rollup -- --config",
    "watch": "npm run compile -- --watch",
    "build": "run-s compile \"web-ext -- build\" \"web-ext -- lint\"",
    "build:chrome": "cross-env BROWSER=chrome npm run build",
    "build:firefox": "cross-env BROWSER=firefox npm run build",
    "png-icons": "flow-node scripts/png-icons.js",
    "rollup": "flow-node node_modules/.bin/rollup",
    "web-ext": "flow-node node_modules/.bin/web-ext",
    "flow": "flow",
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "run-s eslint build:chrome build:firefox"
  },
  "dependencies": {
    "n-ary-huffman": "3.1.0",
    "webextension-polyfill": "0.3.1"
  },
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "cross-env": "5.2.0",
    "cross-spawn": "6.0.5",
    "eslint": "5.5.0",
    "eslint-config-lydell": "10.0.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-sort-imports-es6-autofix": "0.3.0",
    "flow-bin": "0.80.0",
    "mkdirp": "0.5.1",
    "npm-run-all": "4.1.3",
    "prettier": "1.14.2",
    "rimraf": "2.6.2",
    "rollup": "0.65.2",
    "rollup-plugin-commonjs": "9.1.6",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-replace": "2.0.0",
    "web-ext": "2.9.1",
    "write": "1.0.3"
  }
}
