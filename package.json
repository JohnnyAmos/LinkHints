{
  "private": true,
  "scripts": {
    "start": "run-p --race watch firefox",
    "firefox": "npm run web-ext -- run",
    "compile": "npm run rollup -- --config",
    "watch": "npm run compile -- --watch",
    "build": "run-s compile \"web-ext -- build\" \"web-ext -- lint\"",
    "build:chrome": "cross-env BROWSER=chrome npm run build",
    "build:firefox": "cross-env BROWSER=firefox npm run build",
    "png-icons": "flow-node scripts/png-icons.js",
    "rollup": "flow-node node_modules/.bin/rollup",
    "web-ext": "flow-node node_modules/.bin/web-ext",
    "flow": "flow",
    "flow:check": "flow check",
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier --list-different \"*.md\" \"**/*.js\"",
    "prettier:fix": "prettier --write \"*.md\" \"**/*.js\"",
    "test": "run-s eslint prettier flow:check build:chrome build:firefox"
  },
  "dependencies": {
    "n-ary-huffman": "4.0.0",
    "preact": "8.3.1",
    "tiny-decoders": "1.0.0",
    "webextension-polyfill": "0.3.1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "cross-env": "5.2.0",
    "cross-spawn": "6.0.5",
    "eslint": "5.9.0",
    "eslint-config-lydell": "12.0.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-simple-import-sort": "2.0.0",
    "flow-bin": "0.87.0",
    "mkdirp": "0.5.1",
    "npm-run-all": "4.1.5",
    "optional-require": "1.0.0",
    "prettier": "1.15.3",
    "rimraf": "2.6.2",
    "rollup": "0.67.3",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-replace": "2.1.0",
    "web-ext": "2.9.2",
    "write": "1.0.3"
  }
}
