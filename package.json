{
  "private": true,
  "scripts": {
    "firefox": "npm run web-ext -- run",
    "chrome": "npm run web-ext -- run --target chromium",
    "compile": "npm run rollup -- --config",
    "watch": "npm run compile -- --watch",
    "build:chrome": "cross-env BROWSER=chrome run-s compile \"web-ext -- build\" build-post",
    "build:firefox": "cross-env BROWSER=firefox run-s compile \"web-ext -- build\" \"web-ext -- lint\" build-post",
    "build-post": "sucrase-node scripts/build-post.js",
    "png-icons": "sucrase-node scripts/png-icons.js",
    "rollup": "sucrase-node node_modules/.bin/rollup",
    "web-ext": "sucrase-node node_modules/.bin/web-ext",
    "flow": "flow",
    "flow:check": "flow check",
    "eslint": "eslint . --fix",
    "eslint:check": "eslint .",
    "prettier": "prettier --write \"**/*.{css,html,js,md,yml}\"",
    "prettier:check": "prettier --check \"**/*.{css,html,js,md,yml}\"",
    "test": "run-s eslint:check prettier:check flow:check build:chrome build:firefox"
  },
  "dependencies": {
    "n-ary-huffman": "4.0.0",
    "preact": "10.0.5",
    "preact-shadow-root": "1.0.2",
    "tiny-decoders": "4.0.0",
    "webextension-polyfill": "0.5.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "cross-env": "6.0.3",
    "cross-spawn": "7.0.1",
    "crx3": "1.1.1",
    "eslint": "6.7.1",
    "eslint-config-lydell": "14.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "4.5.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-simple-import-sort": "5.0.0",
    "flow-bin": "0.110.1",
    "fs-extra": "8.1.0",
    "js-tokens": "5.0.0",
    "minifycss": "1.0.1",
    "npm-run-all": "4.1.5",
    "optional-require": "1.0.0",
    "preact-render-to-string": "5.1.1",
    "prettier": "1.19.1",
    "readdirp": "3.2.0",
    "rollup": "1.27.5",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-prettier": "0.6.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-sucrase": "2.1.0",
    "web-ext": "3.2.1",
    "yazl": "2.5.1"
  }
}
